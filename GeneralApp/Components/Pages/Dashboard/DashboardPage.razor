@page "/"
@using System.Globalization
@rendermode InteractiveServer

<PageTitle>Panel principal</PageTitle>

<div class="row">
    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-xxl-6">
        <div class="card shadow-sm mb-3 rounded">
            <div class="card-body">
                <GraphLinePage />
            </div>
        </div>
    </div>
    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-xxl-6">
        <div class="card shadow-sm mb-3 rounded">
            <div class="card-body">
                <GrapPiePage />
            </div>
        </div>
    </div>
</div>

<div class="row">
    @for (int i = 0; i < 4; i++)
    {
        <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12 col-xxl-3">
            <CardClipPage />
        </div>
    }
</div>
